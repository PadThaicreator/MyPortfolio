{
  "compilerOptions": {
    "target": "ES2020",                
    "module": "ESNext",                
    "moduleResolution": "node",        
    "esModuleInterop": true,           
    "allowSyntheticDefaultImports": true, 
    "outDir": "./dist",                
    "strict": true,                    
    "skipLibCheck": true,              
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "server/**/*.ts"
  ]
}
